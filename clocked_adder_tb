#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d02cf200d0 .scope module, "clocked_adder_tb" "clocked_adder_tb" 2 1;
 .timescale 0 0;
v0x55d02cf68840_0 .var "A", 3 0;
v0x55d02cf68920_0 .var "B", 3 0;
v0x55d02cf689f0_0 .var "clk", 0 0;
v0x55d02cf68af0_0 .net "sum", 4 0, L_0x55d02cf68bc0;  1 drivers
S_0x55d02cf20250 .scope module, "uut" "clocked_adder" 2 6, 3 1 0, S_0x55d02cf200d0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "A"
    .port_info 1 /INPUT 4 "B"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /OUTPUT 5 "sum"
L_0x55d02cf68bc0 .functor BUFZ 5, v0x55d02cf68690_0, C4<00000>, C4<00000>, C4<00000>;
v0x55d02cf57900_0 .net "A", 3 0, v0x55d02cf68840_0;  1 drivers
v0x55d02cf68400_0 .net "B", 3 0, v0x55d02cf68920_0;  1 drivers
v0x55d02cf684e0_0 .net "clk", 0 0, v0x55d02cf689f0_0;  1 drivers
v0x55d02cf685b0_0 .net "sum", 4 0, L_0x55d02cf68bc0;  alias, 1 drivers
v0x55d02cf68690_0 .var "temp", 4 0;
E_0x55d02cf582b0 .event posedge, v0x55d02cf684e0_0;
    .scope S_0x55d02cf20250;
T_0 ;
    %wait E_0x55d02cf582b0;
    %load/vec4 v0x55d02cf57900_0;
    %pad/u 5;
    %load/vec4 v0x55d02cf68400_0;
    %pad/u 5;
    %add;
    %assign/vec4 v0x55d02cf68690_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d02cf200d0;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "clocked_adder.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d02cf200d0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d02cf689f0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55d02cf68840_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55d02cf68920_0, 0, 4;
    %delay 5, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x55d02cf68840_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x55d02cf68920_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x55d02cf68840_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x55d02cf68920_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x55d02cf68840_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x55d02cf68920_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55d02cf200d0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55d02cf689f0_0;
    %inv;
    %store/vec4 v0x55d02cf689f0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
