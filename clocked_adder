#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x635b1c0cb0f0 .scope module, "clocked_adder_tb" "clocked_adder_tb" 2 3;
 .timescale -9 -12;
v0x635b1c113860_0 .var "A", 3 0;
v0x635b1c113940_0 .var "B", 3 0;
v0x635b1c113a10_0 .var "clk", 0 0;
v0x635b1c113b10_0 .net "sum", 4 0, L_0x635b1c113be0;  1 drivers
S_0x635b1c0cb270 .scope module, "uut" "clocked_adder" 2 8, 3 1 0, S_0x635b1c0cb0f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 4 "A"
    .port_info 1 /INPUT 4 "B"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /OUTPUT 5 "sum"
L_0x635b1c113be0 .functor BUFZ 5, v0x635b1c1136b0_0, C4<00000>, C4<00000>, C4<00000>;
v0x635b1c102920_0 .net "A", 3 0, v0x635b1c113860_0;  1 drivers
v0x635b1c113420_0 .net "B", 3 0, v0x635b1c113940_0;  1 drivers
v0x635b1c113500_0 .net "clk", 0 0, v0x635b1c113a10_0;  1 drivers
v0x635b1c1135d0_0 .net "sum", 4 0, L_0x635b1c113be0;  alias, 1 drivers
v0x635b1c1136b0_0 .var "temp", 4 0;
E_0x635b1c1032d0 .event posedge, v0x635b1c113500_0;
    .scope S_0x635b1c0cb270;
T_0 ;
    %wait E_0x635b1c1032d0;
    %load/vec4 v0x635b1c102920_0;
    %pad/u 5;
    %load/vec4 v0x635b1c113420_0;
    %pad/u 5;
    %add;
    %assign/vec4 v0x635b1c1136b0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x635b1c0cb0f0;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "clocked_adder.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x635b1c0cb0f0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x635b1c113a10_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x635b1c113860_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x635b1c113940_0, 0, 4;
    %delay 5000, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x635b1c113860_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x635b1c113940_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x635b1c113860_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x635b1c113940_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x635b1c113860_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x635b1c113940_0, 0, 4;
    %delay 10000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x635b1c0cb0f0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x635b1c113a10_0;
    %inv;
    %store/vec4 v0x635b1c113a10_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
